{
  "name": "eslint-import-resolver",
  "version": "0.1.0",
  "description": "Universal import resolver",
  "keywords": [
    "eslint",
    "eslint-plugin-import",
    "module",
    "resolver"
  ],
  "main": "dist/eslint-import-resolver.min.js",
  "files": [
    "dist/eslint-import-resolver.min.js",
    "dist/eslint-import-resolver.min.js.map"
  ],
  "scripts": {
    "format": "./node_modules/.bin/eslint --fix src/**/*.js test/**/*.js",
    "build": "./node_modules/.bin/rollup -c rollup.config.js",
    "test": "./node_modules/.bin/nyc --reporter=lcov ./node_modules/.bin/tape -r @babel/register ./test/*.test.js"
  },
  "authors": [
    "Yuriy Yarosh <void.nugget@gmail.com>"
  ],
  "peerDependencies": {
    "lodash": "^4.17.5"
  },
  "dependencies": {
    "lodash-magic-cache": "^0.2.0"
  },
  "license": "MIT",
  "repository": "github:bitsnap/eslint-import-resolver",
  "devDependencies": {
    "lodash": "^4.17.5",
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-external-helpers": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "babel-eslint": "^8.2.2",
    "babel-plugin-lodash-magic-import": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-minify": "^0.4.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-lodash-magic-import": "^0.1.2",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-unicorn": "^4.0.3",
    "nyc": "^11.3.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "tape": "^4.8.0"
  }
}
